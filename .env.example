# Port for the Fastify server
PORT=3001

# MongoDB Connection String
# Example: mongodb://username:password@host:port/database_name
MONGODB_URI=

# OpenSea API Key (Required for collection metadata & events)
# Get from: https://docs.opensea.io/reference/request-an-api-key
OPENSEA_API_KEY=

# Alchemy API Key (Required for NFT floor prices)
# Get from: https://www.alchemy.com/
ALCHEMY_API_KEY=

# CoinGecko API Key (Optional, recommended for ETH price fetching)
# Get from: https://www.coingecko.com/en/api
COINGECKO_API_KEY=

# Infura API Key (Required for gas price fetching)
# Get from: https://www.infura.io/
INFURA_API_KEY=

# Redis Connection URL (Optional, ioredis might use defaults if not set)
# Example: redis://:password@host:port/0
REDIS_URL= 